# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.25.0

# Language settings
language-settings:
  javascript:
    # Ignore dev dependencies in production scans
    ignore-dev-deps: true
    # Set timeout for dependency resolution
    timeout: 300
    # Focus on production dependencies only
    dev-deps: false

# Patch settings
patch: {}

# Ignore settings for known false positives or accepted risks
ignore: {}
